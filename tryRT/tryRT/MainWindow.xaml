                                                        <DataTemplate.Triggers>
                                                            <DataTrigger Binding="{Binding IsSelected,RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type ListBoxItem}}}" Value="True">
                                                                <Setter TargetName="Line" Property="Stroke" Value="Red"/>
                                                            </DataTrigger>
                                                            <DataTrigger Binding="{Binding FullLine}" Value="False">
                                                                <Setter TargetName="Line" Property="StrokeDashArray" Value="2 3"/>
                                                            </DataTrigger>
                                                        </DataTemplate.Triggers>
                                                    </DataTemplate>
                                                    <DataTemplate DataType="{x:Type local:Capacity}">
                                                        <Line x:Name="Line2" Stroke="Red" StrokeThickness="3" X1="{Binding Start.X}" Y1="{Binding Start.Y}" X2="{Binding End.X}" Y2="{Binding Start.Y}"/>
