                                <ListBoxItem Content="kg/s"/>

                            </ListBox>







                        </Canvas>
                    </GroupBox>
                    
                    <!--实时输入显示_End!-->

                    <!-->输入/结果界面_Start<!-->
                    <Canvas HorizontalAlignment="Left" Height="750" VerticalAlignment="Top" Width="445" Margin="200,25,0,0">
                        <TabControl x:Name="TabControl1" HorizontalAlignment="Left" Height="750" VerticalAlignment="Top" Width="450"  Margin="0,0,0,0">

                            <!-->换热器类型_Start<!-->
                            <TabItem x:Name="TabItem_HExType" Header="换热器类型" Visibility="Collapsed">
                                <Grid Background="#FFE5E5E5">
                                    <GroupBox Header="换热器类型"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="80" Width="425" Margin="8,0,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="Auto" Width="Auto" Margin="10,10,0,0" VerticalAlignment="Top">
                                            <Grid>
                                                <ComboBox x:Name="ComboBox_hextype" HorizontalAlignment="Left" Margin="80,5,0,0" VerticalAlignment="Top" Width="150" Height="27" Visibility="Collapsed"/>

                                                <RadioButton GroupName="HExType" Content="冷凝器" x:Name="RadioButton_HExType_Condenser" Margin="0,0,0,0" Width="60" Height="15"  HorizontalAlignment="Left" VerticalAlignment="Top" Checked="HExType_Condenser"/>
                                                <RadioButton GroupName="HExType" Content="蒸发器" x:Name="RadioButton_HExType_Evaporator" Margin="0,20,0,0" Width="60" Height="15"  HorizontalAlignment="Left" VerticalAlignment="Top" Checked="HExType_Evaporator"/>

                                            </Grid>
                                            <Image HorizontalAlignment="Center" Height="Auto" Margin="0,0,0,0" VerticalAlignment="Center" Width="Auto"/>
                                        </Canvas>
                                    </GroupBox>
                                </Grid>
                            </TabItem>
                            <!-->换热管_Start<!-->
                            <TabItem x:Name="TabItem_HExTube" Header="换热管" Visibility="Collapsed">
                                <Grid Background="#FFE5E5E5">

                                    <GroupBox Header="换热管"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="300" Width="425" Margin="8,0,0,0">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="100" Margin="10,8,0,0" VerticalAlignment="Top">
                                            <TextBlock Text="管型号" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="15" Width="36"/>
                                            <ComboBox x:Name="ComboBox_TubeVersion" HorizontalAlignment="Left"  Margin="70,0,0,0" VerticalAlignment="Top" Width="130" Height="26" SelectionChanged="ComboBox_TubeVersion_SelectionChanged"/>

                                            <TextBlock Text="管外径" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Do" Text="10.0584" IsEnabled="False" HorizontalAlignment="Left" Margin="70,30,0,0"  VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,30,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管间距" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Pt" Text="25.4" IsEnabled="False" HorizontalAlignment="Left" Margin="70,60,0,0"  VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,60,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="列间距" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Pr" Text="19.05" IsEnabled="False" HorizontalAlignment="Left" Margin="70,90,0,0"  VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,90,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管排"   TextAlignment="Left" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Row" Text="2" IsEnabled="False" HorizontalAlignment="Left" Margin="70,120,0,0"  VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="R" HorizontalAlignment="Left" Margin="130,120,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管数/排" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Height="15" Width="50"/>
                                            <TextBox x:Name="tube_per" Text="6" HorizontalAlignment="Left" Margin="70,150,0,0"  VerticalAlignment="Top" Height="24" Width="60"/>

                                            <TextBlock Text="管壁厚" HorizontalAlignment="Left" Margin="10,180,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="thick_tube" Text="1.651" HorizontalAlignment="Left" Margin="70,180,0,0" VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,180,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管长" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,210,0,0" VerticalAlignment="Top" Height="14" Width="36"/>
                                            <TextBox x:Name="L" Text="914.4" HorizontalAlignment="Left" Margin="70,210,0,0" VerticalAlignment="Top" Height="24" Width="60"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,210,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管种类" HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top" Height="15" Width="36"/>
                                            <ComboBox x:Name="ComboBox_tubetype" HorizontalAlignment="Left" Margin="70,240,0,0" VerticalAlignment="Top" Width="100" SelectionChanged="ComboBox_tubetype_SelectionChanged"/>
                                        </Canvas>
                                    </GroupBox>
                                    <!-->外形插图_End<!-->
                                    <!-->外形_Start<!-->
                                    <GroupBox Header="外形"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="145" Width="425" Margin="8,300,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="100" Margin="10,10,0,0" VerticalAlignment="Top" ContextMenuClosing="TubeArrangement_Cross_Short_Checked" >
                                            <RadioButton GroupName="TubeArrangement" Content="直排" x:Name="TubeArrangement_Straight" Margin="0,5,0,0" Checked="TubeArrangement_Straight_Checked"/>
                                            <RadioButton GroupName="TubeArrangement" Content="叉排(迎风面高)" x:Name="TubeArrangement_Crossed_High" Margin="0,35,0,0" Checked="TubeArrangement_Cross_High_Checked"/>
                                            <RadioButton GroupName="TubeArrangement" Content="叉排(迎风面低)" x:Name="TubeArrangement_Crossed_Short" Margin="0,65,0,0" Checked="TubeArrangement_Cross_Short_Checked"/>

                                            <!-->外形插图_Start<!-->
                                            <TabControl HorizontalAlignment="Left" Height="100" Margin="180,0,0,0" VerticalAlignment="Top" Width="150">
                                                <TabItem x:Name="TabItem_TubeArrangement_Null" Header="直排" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="467"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Straight" Header="直排" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="467" Source="外形_直排1.png"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Crossed_High" Header="叉排(迎风面高)" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="100" Source="叉排(迎面风高).png"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Crossed_Short" Header="叉排(迎风面低)" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="100" Source="叉排(迎面风低).png"/>
                                                </TabItem>
                                            </TabControl>
                                        </Canvas>
                                    </GroupBox>
                                </Grid>
                            </TabItem>
                            <!-->翅片_Start<!-->
                            <TabItem x:Name="TabItem_Fin" Header="翅片" Visibility="Collapsed">
                                <Grid Background="#FFE5E5E5">

                                    <GroupBox Header="翅片类型"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="120" Width="425" Margin="8,0,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="425" Margin="10,8,0,0" VerticalAlignment="Top">
                                            <TextBlock Text="翅片间距" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <TextBox x:Name="Fnum" Text="10" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Top" Height="25" Width="125"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="230,0,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="翅片厚度" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <TextBox x:Name="Fthick" Text="0.095" HorizontalAlignment="Left" Margin="100,30,0,0" VerticalAlignment="Top" Height="26" Width="125"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="230,30,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="翅片种类" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <ComboBox x:Name="ComboBox_fintype" HorizontalAlignment="Left" Margin="100,60,0,0" VerticalAlignment="Top" Width="125"/>
                                        </Canvas>
                                    </GroupBox>
                                </Grid>
                            </TabItem>
                            <!-->流路_Start<!-->
                            <TabItem x:Name="TabItem_Pass" Header="流路" Visibility="Collapsed">
                                <Grid Background="#FFE5E5E5">

                                    <GroupBox Header="流路" HorizontalAlignment="Left" VerticalAlignment="Top" Height="50" Width="425" Margin="8,0,0,10">
                                        <Grid>
                                            <RadioButton GroupName="PassArrangement" x:Name="RadioButton_ManualArrange" Margin="50,5,0,0" Checked="RadioButton_ManualArrange_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                            <TextBlock Text="手动分配流路" HorizontalAlignment="Left" Margin="70,5,0,0" VerticalAlignment="Top" Width="75"/>

                                            <RadioButton GroupName="PassArrangement" x:Name="RadioButton_AutoArrange" Margin="240,5,0,0" Checked="RadioButton_AutoArrange_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                            <TextBlock Text="自动分配流路" HorizontalAlignment="Left" Margin="260,5,0,0" VerticalAlignment="Top" Width="75"/>
                                        </Grid>
