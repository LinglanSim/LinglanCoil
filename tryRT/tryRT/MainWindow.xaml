                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="管间距:"/>
                                <ListBoxItem Content="{Binding ElementName=Pt,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="列间距:"/>
                                <ListBoxItem Content="{Binding ElementName=Pr,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="------翅片------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="每英尺翅片数"/>
                                <ListBoxItem Content="{Binding ElementName=Fnum,Path=Text}"/>
