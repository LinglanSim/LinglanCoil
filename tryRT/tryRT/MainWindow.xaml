                                <ListBoxItem Content="------流路------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="流路数:"/>
                                <ListBoxItem Content="{Binding ElementName=Cirnum,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="流动形式:"/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_flowtype,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="------制冷剂------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="制冷剂："/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_Refrigerant,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="进口压力:"/>
                                <ListBoxItem Content="{Binding ElementName=Pri_Evap,Path=Text}"/>
                                <ListBoxItem Content="kPa"/>

                                <ListBoxItem Content="进口干度:"/>
                                <ListBoxItem Content="{Binding ElementName=xi_Evap,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="进口焓值:"/>
                                <ListBoxItem Content="{Binding ElementName=Hri_Evap,Path=Text}"/>
                                <ListBoxItem Content="kJ/kg"/>

                                <ListBoxItem Content="进口阀前温度:"/>
                                <ListBoxItem Content="{Binding ElementName=Tri_ValveBefore,Path=Text}"/>
                                <ListBoxItem Content="C"/>

                                <ListBoxItem Content="进口阀前压力:"/>
                                <ListBoxItem Content="{Binding ElementName=Pri_ValveBefore,Path=Text}"/>
                                <ListBoxItem Content="kPa"/>

                                <ListBoxItem Content="出口饱和温度:"/>
                                <ListBoxItem Content="{Binding ElementName=Tcro_Evap,Path=Text}"/>
