                                <ListBoxItem Content="管/排:"/>
                                <ListBoxItem Content="{Binding ElementName=tube_per,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="管排:"/>
                                <ListBoxItem Content="{Binding ElementName=Row,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="管间距:"/>
                                <ListBoxItem Content="{Binding ElementName=Pt,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="列间距:"/>
                                <ListBoxItem Content="{Binding ElementName=Pr,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="------翅片------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="每英尺翅片数"/>
                                <ListBoxItem Content="{Binding ElementName=Fnum,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="翅片厚度："/>
                                <ListBoxItem Content="{Binding ElementName=Fthick,Path=Text}"/>
                                <ListBoxItem Content="mm"/>


                                <ListBoxItem Content="翅片种类："/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_fintype,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="------流路------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="流路数:"/>
                                <ListBoxItem Content="{Binding ElementName=Cirnum,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="流动形式:"/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_flowtype,Path=Text}"/>
                                <ListBoxItem Content=""/>


                                <ListBoxItem Content="------制冷剂------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="制冷剂："/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_Refrigerant,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="进口饱和温度:"/>
                                <ListBoxItem Content="{Binding ElementName=tc,Path=Text}"/>
                                <ListBoxItem Content="C"/>

                                <ListBoxItem Content="进口温度:"/>
                                <ListBoxItem Content="{Binding ElementName=tri,Path=Text}"/>
                                <ListBoxItem Content="C"/>

                                <ListBoxItem Content="出口制冷剂流量:"/>
                                <ListBoxItem Content="{Binding ElementName=mr,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="出口干度:"/>
                                <ListBoxItem Content="{Binding ElementName=x_Cond,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="出口过冷度:"/>
                                <ListBoxItem Content="{Binding ElementName=Tro_ex,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="------风------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="进风温度:"/>
                                <ListBoxItem Content="{Binding ElementName=tai,Path=Text}"/>
                                <ListBoxItem Content="C"/>

                                <ListBoxItem Content="进风相对湿度:"/>
                                <ListBoxItem Content="{Binding ElementName=RHi,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="体积流量:"/>
                                <ListBoxItem Content="{Binding ElementName=Va,Path=Text}"/>
                                <ListBoxItem Content="m3/s"/>
                            </ListBox>

                        </Canvas>
                    </GroupBox>

                    <!--Evaporator!-->
                    <GroupBox x:Name="GroupBox_RealTimeInputShow_Evaporator" Header="当前输入情况" HorizontalAlignment="Right" VerticalAlignment="Top" Height="460" Width="455" Margin="650,275,0,0" Visibility="Hidden">
                        <Canvas HorizontalAlignment="Left" Height="Auto" Margin="0,0,0,0" VerticalAlignment="Top" Width="450">

                            <ListBox Margin="0,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="450" Height="460" ScrollViewer.VerticalScrollBarVisibility="Visible"  ScrollViewer.HorizontalScrollBarVisibility="Disabled">

                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <UniformGrid Columns="3"/>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>


                                <ListBoxItem Content="换热器类型:"/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_hextype,Path=Text}"/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="------换热管------" FontWeight="Bold"/>
                                <ListBoxItem Content=""/>
                                <ListBoxItem Content=""/>

                                <ListBoxItem Content="管型号:"/>
                                <ListBoxItem Content="{Binding ElementName=ComboBox_TubeVersion,Path=Text}"/>
