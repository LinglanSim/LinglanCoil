                                <ListBoxItem Content="管壁厚:"/>
                                <ListBoxItem Content="{Binding ElementName=thick_tube,Path=Text}"/>
                                <ListBoxItem Content="mm"/>

                                <ListBoxItem Content="管长:"/>
                                <ListBoxItem Content="{Binding ElementName=L,Path=Text}"/>
