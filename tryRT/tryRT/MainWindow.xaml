<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:tryRT"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="tryRT.MainWindow"
        xmlns:input="clr-namespace:System.Windows.Input;assembly=PresentationCore"
        Title="美的换热器仿真" Height="820" Width="1150" x:Name="view" WindowStartupLocation="CenterScreen">

    <!--<ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Auto">-->
        <!--<Viewbox Stretch="Fill">-->
            <Grid Height="Auto" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <Grid.RowDefinitions>
                    <RowDefinition Height="25"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <!-->第1部分<!-->
                <!-->菜单栏<!-->
                <Grid Grid.Row="0" Height="25" VerticalAlignment="Top" Width="Auto" HorizontalAlignment="Stretch" Margin="0,0,0,0">
                    <Menu Height="25" Grid.RowSpan="1" VerticalAlignment="Top" Width="Auto" HorizontalAlignment="Stretch"  Margin="0,0,0,0" Background="#FFE5E5E5">
                        <MenuItem Header="文件">
                            <MenuItem.Icon>
                                <Image Source="Pictures\菜单_文件.PNG"/>
                            </MenuItem.Icon>
                        <MenuItem Header="导入输入文件" Click="MenuItem_Click_InportInput"/>
                        <MenuItem Header="导出当前输入" Click="MenuItem_Click_ExportInput"/>
                            <MenuItem Header="退出" Click="MenuItem_Click_Close">
                                <MenuItem.Icon>
                                    <Image Height="25" Width="28" Source="Pictures\菜单_Close.png"/>
                                </MenuItem.Icon>
                            </MenuItem>

                        </MenuItem>
                        <MenuItem Header="仿真">
                            <MenuItem.Icon>
                                <Image Source="Pictures\菜单_Run.PNG"/>
                            </MenuItem.Icon>
                            <MenuItem Header="开始运行"  Click="MenuItem_Click_Calculate">
                                <MenuItem.Icon>
                                    <Image Height="25" Width="28" Source="Pictures\菜单_运行.PNG" />
                                </MenuItem.Icon>
                            </MenuItem>
                        </MenuItem>
                        <MenuItem Header="恢复默认设置">
                            <MenuItem.Icon>
                                <Image Height="20" Width="20" Source="Pictures/菜单_默认设置.jpg" />
                            </MenuItem.Icon>
                            <MenuItem Header="冷凝器" Click="MenuItem_Click_DefaultCond"/>
                            <MenuItem Header="蒸发器" Click="MenuItem_Click_DefaultEvap" />
                        </MenuItem>
                        <MenuItem Header="帮助">
                            <MenuItem.Icon>
                                <Image Source="Pictures\菜单_Help.PNG" />
                            </MenuItem.Icon>
                            <MenuItem Header="关于我们"/>
                        </MenuItem>
                    </Menu>
                </Grid>

                <!-->第2部分<!-->
                <Grid Grid.Row="1" Margin="0,0,0,0" VerticalAlignment="Stretch" Height="Auto">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition MaxWidth="200" MinWidth="40" Width="150*"/>
                        <ColumnDefinition Width="1000*"/>
                    </Grid.ColumnDefinitions>

                    <!-->第2.1部分<!-->
                    <!--Tree View!-->
                    <DockPanel Grid.Column="0" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,0,0">
                        <TreeView HorizontalAlignment="Stretch"  Height="Auto" VerticalAlignment="Stretch" BorderThickness="0">
                            <TreeViewItem Header="输入" IsExpanded="True" Width="Auto" HorizontalAlignment="Left" Height="Auto" VerticalAlignment="Top">
                                <TreeViewItem x:Name="TreeView_HExType" Header="类型" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_HExType_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_HExTube" Header="换热管" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_HExTube_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_Fin" Header="翅片" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_Fin_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_Ref" Header="制冷剂侧" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_Ref_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_Wind" Header="空气侧" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_Wind_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_Pass" Header="流路" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_Pass_GotFocus"/>
                                <TreeViewItem x:Name="TreeView_AdjustParameter" Header="修正系数" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_AdjustParameter_GotFocus"/>
                            </TreeViewItem>
                            <TreeViewItem Header="输出" IsExpanded="True">
                                <TreeViewItem x:Name="TreeView_DetailResult" Header="具体计算结果" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_DetailResult_GotFocus">
                                    <TreeViewItem x:Name="TreeView_DetailResultExcelOutput" Header="输出Excel格式" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_DetailResultExcelOutput_GotFocus"/>
                                </TreeViewItem>
                                <TreeViewItem x:Name="TreeView_Result" Header="计算结果" HorizontalAlignment="Left" Width="Auto" GotFocus="TreeView_Result_GotFocus"/>
                            </TreeViewItem>
                        </TreeView>
                    </DockPanel>

                    <GridSplitter Background="Gray" Width="2" Margin="0,0,0,0"></GridSplitter>

                    <!-->第2.2部分<!-->
                    <Grid Grid.Column="1" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="Grid_Mid" Width="430*" MinWidth="50"/>
                            <ColumnDefinition x:Name="Grid_Right" Width="570*" MinWidth="50"/>
                        </Grid.ColumnDefinitions>
                        
                        <!-->第2.2.1部分<!-->                                                
                        <ScrollViewer Grid.Column="0" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Disabled" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#FFE5E5E5">
                            <Grid Width="430" MinWidth="50" Height="Auto" HorizontalAlignment="Left">
                                <!-->换热器仿真<!-->
                                <Canvas x:Name="Canvas_0" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#FFE5E5E5" Visibility="Visible">
                                    <Label Content="美的换热器仿真" HorizontalAlignment="Center" Margin="180,60,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="24"/>
                                    <Image HorizontalAlignment="Center" Height="65" Margin="80,50,0,0" VerticalAlignment="Top" Width="Auto" Source="Pictures\美的Logo.png"/>
                                    <GroupBox Header="换热器类型"  HorizontalAlignment="Center" VerticalAlignment="Top" Height="100" Width="140" Margin="150,110,0,0">
                                        <Canvas HorizontalAlignment="Center" Height="Auto" Width="Auto" Margin="10,10,0,0" VerticalAlignment="Top">
                                            <Grid>
                                                <ComboBox x:Name="ComboBox_hextype" HorizontalAlignment="Left" Margin="80,5,0,0" VerticalAlignment="Top" Width="150" Height="27" Visibility="Collapsed"/>
                                                <RadioButton GroupName="HExType" Content="冷凝器" x:Name="RadioButton_HExType_Condenser" Margin="-40,0,0,0" Width="60" Height="15"  HorizontalAlignment="Center" VerticalAlignment="Top" Checked="HExType_Condenser"/>
                                                <RadioButton GroupName="HExType" Content="蒸发器" x:Name="RadioButton_HExType_Evaporator" Margin="-40,40,0,0" Width="60" Height="15"  HorizontalAlignment="Center" VerticalAlignment="Top" Checked="HExType_Evaporator"/>
                                            </Grid>
                                        </Canvas>
                                    </GroupBox>
                                </Canvas>

                                <!-->换热管<!-->
                                <Canvas x:Name="Canvas_1" Width="Auto" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="#FFE5E5E5" Visibility="Collapsed">
                                    <GroupBox Header="换热管"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="260" Width="410" Margin="8,0,0,0">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="100" Margin="10,8,0,0" VerticalAlignment="Top">
                                            <TextBlock Text="管型号" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="15" Width="36"/>
                                            <ComboBox x:Name="ComboBox_TubeVersion" HorizontalAlignment="Left"  Margin="70,0,0,0" VerticalAlignment="Top" Width="140" Height="26" SelectionChanged="ComboBox_TubeVersion_SelectionChanged"/>

                                            <TextBlock Text="管外径" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Do" Text="10.0584" IsEnabled="False" HorizontalAlignment="Left" Margin="70,40,0,0" VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,40,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管间距" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Pt" Text="25.4" IsEnabled="False" HorizontalAlignment="Left" Margin="70,80,0,0"  VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,80,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="列间距" HorizontalAlignment="Left" Margin="220,80,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="Pr" Text="19.05" IsEnabled="False" HorizontalAlignment="Left" Margin="280,80,0,0"  VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="340,80,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管排"   TextAlignment="Left" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Height="16" Width="36" />
                                            <TextBox x:Name="Row" Text="2" IsEnabled="False" HorizontalAlignment="Left" Margin="70,120,0,0"  VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="NumTextBox_PreviewTextInput" TextChanged="NumTextBox_TextChanged"/>
                                            <Label Content="#" HorizontalAlignment="Left" Margin="130,120,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管数/排" HorizontalAlignment="Left" Margin="220,120,0,0" VerticalAlignment="Top" Height="15" Width="50"/>
                                            <TextBox x:Name="tube_per" Text="6" HorizontalAlignment="Left" Margin="280,120,0,0"  VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="NumTextBox_PreviewTextInput" TextChanged="NumTextBox_TextChanged"/>
                                            <Label Content="#" HorizontalAlignment="Left" Margin="340,120,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管壁厚" HorizontalAlignment="Left" Margin="220,160,0,0" VerticalAlignment="Top" Height="16" Width="36"/>
                                            <TextBox x:Name="thick_tube" Text="0.2" HorizontalAlignment="Left" Margin="280,160,0,0" VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="340,160,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管长" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Height="14" Width="36"/>
                                            <TextBox x:Name="L" Text="914.4" HorizontalAlignment="Left" Margin="70,160,0,0" VerticalAlignment="Top" Height="24" Width="60" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="130,160,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="管种类" HorizontalAlignment="Left" Margin="220,40,0,0" VerticalAlignment="Top" Height="15" Width="36"/>
                                            <ComboBox x:Name="ComboBox_tubetype" HorizontalAlignment="Left" Margin="280,40,0,0" VerticalAlignment="Top" Width="100"  Height="26" SelectionChanged="ComboBox_tubetype_SelectionChanged"/>

                                            <Button x:Name="Button1" Content="确定" HorizontalAlignment="Stretch" Margin="130,200,5,5" Width="120" Click="Button_Click_Sure" Visibility="Visible"></Button>
                                        </Canvas>
                                    </GroupBox>                               
                                    <!-->外形_Start<!-->
                                    <GroupBox Header="外形"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="290" Width="410" Margin="8,265,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="100" Margin="10,10,0,0" VerticalAlignment="Top" ContextMenuClosing="TubeArrangement_Cross_Short_Checked" >
                                            <RadioButton GroupName="TubeArrangement" Content="直排" x:Name="TubeArrangement_Straight" Margin="0,5,0,0" Checked="TubeArrangement_Straight_Checked"/>
                                            <RadioButton GroupName="TubeArrangement" Content="叉排(迎风面高)" x:Name="TubeArrangement_Crossed_High" Margin="0,35,0,0" Checked="TubeArrangement_Cross_High_Checked"/>
                                            <RadioButton GroupName="TubeArrangement" Content="叉排(迎风面低)" x:Name="TubeArrangement_Crossed_Short" Margin="0,65,0,0" Checked="TubeArrangement_Cross_Short_Checked"/>
                                            <!-->外形插图_Start<!-->
                                            <TabControl HorizontalAlignment="Left" Height="250" Margin="130,-5,0,0" VerticalAlignment="Top" Width="250">
                                                <TabItem x:Name="TabItem_TubeArrangement_Null" Header="Null" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="467"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Straight" Header="直排" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Center" Height="230" Margin="0,0,0,0" VerticalAlignment="Top" Width="500" Source="Pictures\直排.PNG"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Crossed_High" Header="叉排(迎风面高)" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Center" Height="230" Margin="0,0,0,0" VerticalAlignment="Top" Width="500" Source="Pictures\叉排迎风面高.PNG"/>
                                                </TabItem>
                                                <TabItem x:Name="TabItem_TubeArrangement_Crossed_Short" Header="叉排(迎风面低)" Visibility="Collapsed">
                                                    <Image HorizontalAlignment="Center" Height="230" Margin="0,0,0,0" VerticalAlignment="Top" Width="500" Source="Pictures\叉排迎风面低.PNG"/>
                                                </TabItem>
                                            </TabControl>
                                        </Canvas>
                                    </GroupBox>
                                </Canvas>

                                <!-->翅片<!-->
                                <Canvas x:Name="Canvas_2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" Background="#FFE5E5E5">
                                    <GroupBox Header="翅片类型"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="140" Width="410" Margin="8,0,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="425" Margin="10,8,0,0" VerticalAlignment="Top">
                                            <TextBlock Text="翅片间距" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <TextBox x:Name="Fnum" Text="1.2" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Top" Height="25" Width="125" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="230,0,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="翅片厚度" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <TextBox x:Name="Fthick" Text="0.095" HorizontalAlignment="Left" Margin="100,40,0,0" VerticalAlignment="Top" Height="26" Width="125" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                            <Label Content="mm" HorizontalAlignment="Left" Margin="230,40,0,0" VerticalAlignment="Top"/>

                                            <TextBlock Text="翅片种类" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Height="22" Width="80"/>
                                            <ComboBox x:Name="ComboBox_fintype" HorizontalAlignment="Left" Margin="100,80,0,0" VerticalAlignment="Top" Width="125"/>
                                        </Canvas>
                                    </GroupBox>
                                </Canvas>
                                <!-->制冷剂侧<!-->
                                <Canvas x:Name="Canvas_3" Width="Auto" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed" Background="#FFE5E5E5">
                                    <GroupBox Header="制冷剂侧"  HorizontalAlignment="Left" VerticalAlignment="Top" Height="70" Width="410" Margin="8,0,0,10">
                                        <Canvas HorizontalAlignment="Left" Height="480" Width="100" Margin="10,8,0,0" VerticalAlignment="Top">
                                            <TextBlock Text="制冷剂" TextAlignment="Left" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="60"/>
                                            <ComboBox x:Name="ComboBox_Refrigerant" HorizontalAlignment="Left" Margin="70,0,0,0" VerticalAlignment="Top" Width="152" SelectionChanged="ComboBox_Refrigerant_SelectionChanged" RenderTransformOrigin="0.067,1.318"/>
                                        </Canvas>
                                    </GroupBox>
                                    <Canvas HorizontalAlignment="Left" Height="100" Margin="0,60,0,0" VerticalAlignment="Top" Width="100">
                                        <!--蒸发器_Start!-->
                                        <!--新加入蒸发器20180808!-->

                                        <!--进口参数_Start!-->
                                        <Canvas HorizontalAlignment="Left" Height="100" Margin="8,0,0,0" VerticalAlignment="Top" Width="120">
                                            <GroupBox Header="进口参数" x:Name="GroupBox_RefInlet_Evaporator" HorizontalAlignment="Left"  Margin="0,10,0,0" Width="200" Height="220"  Visibility="Hidden">
                                                <Canvas HorizontalAlignment="Left" Height="80" Margin="0,0,0,0" VerticalAlignment="Top" Width="Auto">

                                                    <RadioButton GroupName="Pri+Tcro_Evap" x:Name="RadioButton_Pri_Evap" Margin="5,10,0,0" Checked="RadioButton_Pri_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="压力" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="Pri_Evap" Text="" HorizontalAlignment="Left" Height="24" Margin="75,10,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="TextBox_PreviewTextInput" TextChanged="TextBox_TextChanged"/>
                                                    <Label Content="kPa" HorizontalAlignment="Left" Margin="150,10,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="xi+Hri+Tri+Pri_Evap" x:Name="RadioButton_xi_Evap" Margin="5,50,0,0" Checked="RadioButton_xi_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="干度" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,50,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="xi_Evap" Text="" HorizontalAlignment="Left" Height="24" Margin="75,50,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                                    <Label Content="" HorizontalAlignment="Left" Margin="150,50,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="xi+Hri+Tri+Pri_Evap" x:Name="RadioButton_Hri_Evap" Margin="5,90,0,0" Checked="RadioButton_Hri_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="焓值" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,90,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="Hri_Evap" Text="" HorizontalAlignment="Left" Height="24" Margin="75,90,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="TextBox_PreviewTextInput" TextChanged="TextBox_TextChanged"/>
                                                    <Label Content="kJ/kg" HorizontalAlignment="Left" Margin="150,90,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="xi+Hri+Tri+Pri_Evap" x:Name="RadioButton_PriTri_Evap" Margin="5,130,0,0" Checked="RadioButton_PriTri_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="阀前温度" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,130,0,0" VerticalAlignment="Top" Width="60" />
                                                    <TextBox x:Name="Tri_ValveBefore"  HorizontalAlignment="Left" Height="24" Margin="75,130,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="TextBox_PreviewTextInput" TextChanged="TextBox_TextChanged"/>
                                                    <Label Content="C" HorizontalAlignment="Left" Margin="150,130,0,0" VerticalAlignment="Top"/>

                                                    <TextBlock Text="阀前压力" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,170,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="Pri_ValveBefore" HorizontalAlignment="Left" Height="24" Margin="75,170,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="TextBox_PreviewTextInput" TextChanged="TextBox_TextChanged"/>
                                                    <Label Content="kPa" HorizontalAlignment="Left" Margin="150,170,0,0" VerticalAlignment="Top"/>

                                                </Canvas>
                                            </GroupBox>
                                        </Canvas>
                                        <!--出口参数_Start!-->
                                        <Canvas HorizontalAlignment="Left" x:Name="GroupBox_RefOutlet_Evaporator" Height="100" Margin="220,0,0,0" VerticalAlignment="Top" Width="100"  Visibility="Hidden">
                                            <GroupBox Header="出口参数" HorizontalAlignment="Left"  Margin="0,10,0,0" Width="200" Height="220">
                                                <Canvas HorizontalAlignment="Left" Height="80" Margin="0,0,0,0" VerticalAlignment="Top" Width="Auto">

                                                    <RadioButton GroupName="Pri+Tcro_Evap" x:Name="RadioButton_Tro_Evap" Margin="5,10,0,0" Checked="RadioButton_Tcro_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="饱和温度" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="Tcro_Evap" Text="10" HorizontalAlignment="Left" Height="24" Margin="75,10,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="TextBox_PreviewTextInput" TextChanged="TextBox_TextChanged"/>
                                                    <Label Content="C" HorizontalAlignment="Left" Margin="150,10,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="Troex+xo+mro_Evap" x:Name="RadioButton_Tro_sub_Evap" Margin="5,50,0,0" Checked="RadioButton_Troex_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="过热度" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,50,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="Tro_sub_Evap" Text="" HorizontalAlignment="Left" Height="24" Margin="75,50,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                                    <Label Content="" HorizontalAlignment="Left" Margin="150,50,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="Troex+xo+mro_Evap" x:Name="RadioButton_xo_Evap" Margin="5,90,0,0" Checked="RadioButton_xo_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="干度" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,90,0,0" VerticalAlignment="Top" Width="60"/>
                                                    <TextBox x:Name="xo_Evap" Text="" HorizontalAlignment="Left" Height="24" Margin="75,90,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                                    <Label Content="" HorizontalAlignment="Left" Margin="150,90,0,0" VerticalAlignment="Top"/>

                                                    <RadioButton GroupName="Troex+xo+mro_Evap" x:Name="RadioButton_mro_Evap" Margin="5,130,0,0" Checked="RadioButton_mro_Evap_Checked" Width="12" Height="12" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                    <TextBlock Text="流量" TextAlignment="Left" HorizontalAlignment="Left" Margin="20,130,0,0" VerticalAlignment="Top" Width="60" />
                                                    <TextBox x:Name="mro_Evap" Text="0.02" HorizontalAlignment="Left" Height="24" Margin="75,130,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False" input:InputMethod.IsInputMethodEnabled="False" PreviewTextInput="PositiveTextBox_PreviewTextInput" TextChanged="PositiveTextBox_TextChanged"/>
                                                    <Label Content="kg/s" HorizontalAlignment="Left" Margin="150,130,0,0" VerticalAlignment="Top"/>
                                                </Canvas>
                                            </GroupBox>
                                        </Canvas>

                                        <!--冷凝器_Start!-->
                                        <!--进口参数_Start!-->
                                        <Canvas HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="200">
